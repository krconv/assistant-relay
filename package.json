  },
  "homepage": "https://github.com/krconv/assistant-relay#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "google-assistant": "^0.6.0",
    "helmet": "^3.21.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1"
  },
  "devDependencies": {
